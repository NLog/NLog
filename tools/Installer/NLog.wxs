      <Custom Action="VS2005InstallVSTemplates" Before="InstallFinalize">NOT (&amp;Feature_VS2005 = -1)</Custom>
      <Custom Action="VCSHARP2005InstallVSTemplates" Before="InstallFinalize">NOT (&amp;Feature_VCSHARP2005 = -1)</Custom>
