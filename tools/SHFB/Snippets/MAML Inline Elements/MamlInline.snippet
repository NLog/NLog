		</Header>
		<Snippet>
			<Declarations>
				<Literal Editable="true">
					<ID>address</ID>
					<ToolTip>The in-page link address.</ToolTip>
					<Default>elementAddress</Default>
				</Literal>
				<Literal Editable="true">
					<ID>innerText</ID>
					<ToolTip>The link inner text.</ToolTip>
					<Default xml:space="preserve"> Inner text</Default>
				</Literal>
			</Declarations>
			<Code Language="XML">
				<![CDATA[<link xlink:href="#$address$">$selected$$innerText$</link>$end$]]>
			</Code>
		</Snippet>
	</CodeSnippet>
