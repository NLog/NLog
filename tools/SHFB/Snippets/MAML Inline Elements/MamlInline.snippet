	<!-- externalLink element -->
	<CodeSnippet Format="1.0.0">
		<Header>
			<SnippetTypes>
				<SnippetType>Expansion</SnippetType>
				<SnippetType>SurroundsWith</SnippetType>
			</SnippetTypes>
			<Title>externalLink element</Title>
			<Shortcut>externalLinkn</Shortcut>
			<Description>The externalLink inline element</Description>
			<Author>Eric Woodruff</Author>
		</Header>
		<Snippet>
			<Declarations>
				<Literal Editable="true">
					<ID>linkText</ID>
					<ToolTip>The link text.</ToolTip>
					<Default xml:space="preserve"> Inner text</Default>
				</Literal>
				<Literal Editable="true">
					<ID>linkAlternateText</ID>
					<ToolTip>The optional link alternate text.  Remove the element if not needed.</ToolTip>
					<Default>Optional alternate text</Default>
				</Literal>
				<Literal Editable="true">
					<ID>linkUri</ID>
					<ToolTip>The URL for the external link.</ToolTip>
					<Default>https://GitHub.com/EWSoftware/SHFB</Default>
				</Literal>
				<Literal Editable="true">
					<ID>linkTarget</ID>
					<ToolTip>The optional link target.  Remove the element if not needed.</ToolTip>
					<Default>_blank</Default>
				</Literal>
			</Declarations>
			<Code Language="XML">
				<![CDATA[<externalLink>
	<linkText>$selected$$linkText$</linkText>
	<linkAlternateText>$linkAlternateText$</linkAlternateText>
  <linkUri>$linkUri$</linkUri>
	<linkTarget>$linkTarget$</linkTarget>
</externalLink>$end$]]>
			</Code>
		</Snippet>
	</CodeSnippet>
