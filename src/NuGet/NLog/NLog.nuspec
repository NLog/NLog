    <title>NLog</title>
    <authors>Kim Christensen, Julian Verdurmen</authors>
        <owners>Julian Verdurmen</owners>
        <licenseUrl>http://raw.github.com/NLog/NLog/master/LICENSE.txt</licenseUrl>
