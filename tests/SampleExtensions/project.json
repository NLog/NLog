{
  "version": "4.4.0-beta1",
  "description": "NLog Library",
  "authors": [ "NLog" ],
  "packOptions": {
    "tags": [ "NLog" ],
    "projectUrl": "",
    "licenseUrl": "",
    "repository": {
      "type": "git",
      "url": "git://github.com/NLog/NLog"
    }
  },

  "dependencies": {
    "NLog": {
      "target": "project"
    }
  },

  "frameworks": {
