{
    "version": "4.4.0-alpha1",
    "description": "NLog Library",
    "authors": [ "NLog" ],
    "packOptions": {
        "tags": [ "NLog" ],
        "projectUrl": "",
        "licenseUrl": "",
        "repository": {
            "type": "git",
            "url": "git://github.com/NLog/NLog"
        }
    },
    "dependencies": {
        "NLog": "4.4.0-beta9",
        "SampleExtensions": "4.*"
    },
    "testRunner": "xunit",

    "frameworks": {
      
        "netcoreapp1.0": {
            "dependencies": {
                "Microsoft.NETCore.App": {
                    "type": "platform",
                    "version": "1.0.0-rc2-3002702"
                },
                "dotnet-test-xunit": "1.0.0-rc2-build10025",
                "xunit": "2.1.0",
                "Microsoft.AspNetCore.Mvc.WebApiCompatShim": "1.0.0-rc2-final",
                "System.Threading.Tasks.Parallel": "4.0.1-rc2-24027",
                "System.Xml.XDocument": "4.0.11-rc2-24027",
                "System.Data.SqlClient": "4.1.0-rc2-24027"
            },
            "buildOptions": {
                "define": [ "XUNIT2", "NET4_5", "NETSTANDARD_1plus", "NETSTANDARD1_3", "ASYNC_SUPPORTED", "UWP10" ]
            },
            "imports": [
                "dnxcore50",
                "portable-net45+win8"
            ]

        }
    },
    "buildOptions": {
        "keyFile": "NLogTests.snk",
        "nowarn": [ ]
    }
   
}
